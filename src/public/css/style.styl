colors = (curious #3399CC) (turq #33CCCC) (strikemaster #996699) (crail #C24747) (terracotta #E2674A) (golden #FFCC66) (york #99CC99) (patina #669999) (hopbush #CC6699) (sea #339966)   (kimberly #666699)

render(colors)
    for color in colors
        &.{color[0]}
            for percentage, i in 0% 9% 18% 27% 36% 45% 54% 63%
                &.variant-{i}
                    bg = darken(color[1], percentage)
                    background: bg
                    if light(darken(bg, 10%))
                        color: #333

.style
    render(colors)

gradient(a, b)
    background: a
    background: -moz-linear-gradient(top, a 0%, b 100%)
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,a), color-stop(100%,b))
    background: -webkit-linear-gradient(top, a 0%, b 100%)
    background: -o-linear-gradient(top, a 0%, b 100%)
    background: -ms-linear-gradient(top, a 0%, b 100%)
    background: linear-gradient(to bottom, a 0%, b 100%)

