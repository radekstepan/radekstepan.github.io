html
    height: 100%
    font-size: 100%

    &:not(.detail)
        overflow-y: hidden

*,
*:after,
*:before
    -webkit-box-sizing: border-box
    -moz-box-sizing: border-box
    box-sizing: border-box

.clearfix
    *zoom: 1

    &:before, &:after
        content: " "
        display: table
    &:after
        clear: both

:focus
    outline:none !important

body
    font-family: 'Lato', Calibri, Arial, sans-serif
    background: #f9f9f9
    font-weight: 400
    font-size: 1em
    color: #333
    overflow: scroll
    overflow-x: hidden
    margin: 0

a
    cursor: pointer
    text-decoration: none

#main
    width: 90%
    max-width: 76em
    margin: 0 auto
    padding: 0 1.875em 0 1.875em
    position: relative

.container
    width: 100%
    position: relative
    background: #FFF

    > header
        width: 90%
        max-width: 72em
        margin: 0 auto
        padding: 1.875em


        h1
            font-size: 2.125em
            line-height: 1.3
            margin: 0
            font-weight: 700
            color: #333
            float: left

            span
                display: block
                font-size: 65%
                font-weight: 400
                color: #666

#top
    line-height: 3.5
    font-size: 0.688em
    background: url('../img/irongrip.png')
    text-transform: uppercase
    z-index: 9999
    position: relative

    a
        padding: 0 1em
        letter-spacing: 0.1em
        color: #FFF
        display: inline-block

        &:before
            font-family: "EntypoSocial"
            display: inline-block
            font-size: 2.5em
            line-height: 1em
            margin-right: 0.1em
            margin-bottom: 0.21em
            vertical-align: bottom

        &.github:before
            content: "\F300"

        &.behance:before
            content: "\F34E"

        &.linkedin:before
            content: "\F318"

        &:hover
            background: rgba(255,255,255,0.1)

    span.right
        float: right

        a
            float: left
            display: block

#categories
    float: right
    padding-top: 0.625em

    a
        display: inline-block
        margin: 0.625em
        color: #666
        font-weight: 700
        font-size: 90%
        line-height: 2
        border-bottom: 4px solid transparent

        &:last-child
            margin-right: 0

        &:hover
            color: #000
            border-color: #000

        &.current, &.current:hover
            color: #aaa
            border-color: #aaa

.support-note
    clear: both
    
    span
        color: #ac375d
        display: none
        font-weight: bold
        text-align: center
        padding: 0.313em 0

.rb-grid
    text-align: center
    margin: 0 auto

    > li
        width: 24%
        height: 7em
        margin: 0 0.5% 1% 0.5%
        color: #fff
        display: block
        float: left
        padding: 1em
        cursor: pointer
        position: relative
        overflow: hidden

        &[class^="entypo-"]::before, &[class*=" entypo-"]::before
            font-family: 'Entypo'
            display: block
            position: absolute
            bottom: 0
            right: 0
            font-size: 13em
            line-height: 0.3em
            opacity: 0.15

        &.entypo-heart-empty::before
            content: '\2661'
        &.entypo-graduation-cap::before
            content: '\1F393'
        &.entypo-upload-cloud::before
            content: '\E711'
        &.entypo-thumbs-down::before
            content: '\1F44E'
        &.entypo-briefcase::before
            content: '\1F4BC'
        &.entypo-bar-graph::before
            content: '\1F4CA'
        &.entypo-light-bulb::before
            content: '\1F4A1'

        > a
            background: inherit
            text-decoration: none
            color: inherit

        &.disabled
            cursor: default

        &.span-2
            width: 49%

        &.span-4
            width: 76%

        span.title
            display: block
            margin: 0 0 0.2em 0
            font-size: 2em
            font-weight: 400
            opacity: 0.7
            -webkit-transition: opacity 0.3s ease-in-out
            -moz-transition: opacity 0.3s ease-in-out
            transition: opacity 0.3s ease-in-out

            for i in 0...10
                &.size-{i}
                    font-size: 2 - i * 0.2  em

        span.subtitle
            display: block
            margin: 0
            font-size: 0.8em
            font-weight: 400
            opacity: 0.8

        &[class^="icon-"]:before, &[class*=" icon-"]:before
            font-size: 10em
            position: absolute
            display: block
            width: 100%
            height: 100%
            top: 0
            left: 0
            line-height: 3
            opacity: 0.4
            text-align: right
            pointer-events: none
        
        &:hover span.title
            opacity: 1

.rb-overlay
    opacity: 0
    position: fixed
    top: 0
    left: 0
    width: 100%
    height: 100%
    background: inherit
    -webkit-transition: opacity 0.4s ease, -webkit-transform 0.4s ease, clip 0.4s ease
    -moz-transition: opacity 0.4s ease, -moz-transform 0.4s ease, clip 0.4s ease
    transition: opacity 0.4s ease, transform 0.4s ease, clip 0.4s ease
    z-index: -1
    pointer-events: none
    cursor: default

.rb-close
    position: absolute
    top: 0.4em
    right: 0.4em
    width: 2em
    height: 2em
    text-indent: -9000px
    cursor: pointer
    z-index: 1000

    &::before
        content: 'x'
        font-weight: 100
        position: absolute
        top: 0
        left: 0
        width: 100%
        height: 100%
        font-size: 3em
        line-height: 0.6
        text-align: center
        text-indent: 0px

.rb-content
    width: 100%
    height: 100%
    text-align: left
    color: #FFF

    .wrap
        width: 50%
        margin: 4em
        padding: 4em
        background: rgba(0,0,0,0.8)

    h3
        font-size: 4em
        font-weight: 700
        margin: 0 0 0.5em 0

    p, ul, ol
        line-height: 1.5em
        margin-top: 1em

    a
        color: #FFF
        text-decoration: underline

        &.github
            font-size: 1.5em
            text-align: middle
            text-decoration: none

            &:before
                font-family: "EntypoSocial"
                content: "\f300"
                font-size: 2em
                margin-right: 5px

    img
        width: 100%
        position: absolute
        top: 0
        left: 0
        opacity: 0.2
        z-index: -1

    [class^="icon-"]:before
        font-size: 2.5em
        font-weight: normal

// jScrollPane.
.jspContainer
    overflow: hidden
    position: relative

    .jspPane
        position: absolute
        width: 100% !important
        left: 0 !important

        > *
            padding: 0 23px

    &:hover .jspVerticalBar
        opacity: 1

    .jspVerticalBar
        position: absolute
        top: 0px
        right: 1%
        width: 9px
        height: 100%
        // opacity: 0
        -webkit-transition: opacity 0.2s linear
        z-index: 2

        .jspCap
            height: 5px
            display: block

        .jspTrack
            background: rgba(242,242,242,0.5)
            box-shadow: inset 0 0 1px rgba(0,0,0,0.1), inset 1px 0 4px rgba(0,0,0,0.05)
            border-radius: 10px
            position: relative

            .jspDrag
                background: rgba(57,57,57,0.2)
                position: relative
                box-shadow: inset -1px 0 8px rgba(0,0,0,0.05)
                top: 0
                left: 0
                border-radius: 5px
                border: 1px solid #f0f0f0
                cursor: pointer

    .jspCorner
        background: #fff
        float: left
        height: 100%

.rb-content
    .jspContainer
        .jspVerticalBar
            .jspTrack
                .jspDrag
                    background: #fff

@media screen and (max-width: 63.125em)
    .rb-grid li,
    .rb-grid li.span-2,
    .rb-grid li.span-4
        width: 100%
        text-align: left